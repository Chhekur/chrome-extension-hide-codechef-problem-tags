{
    "manifest_version": 2,
   
    "name": "Hide CodeChef Problem Tags",
    "version": "1.0.0",
    "description": "Hides the tags of problem on codechef",
   
    "content_scripts": [{
      "js": ["js/main.js"],
      "matches": ["https://www.codechef.com/*"]
    }],

    "browser_action":{
        "default_icon": "icons/icon1.png"
    },

    "icons":{
        "16": "icons/icon1.png",
        "48": "icons/icon1.png",
        "128": "icons/icon1.png"
    }
   
  }